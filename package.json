{
  "name": "marvel-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "./node_modules/.bin/env-cmd ./node_modules/.bin/nodemon index.js",
    "dev:client": "cd client && npm start",
    "dev:client:build": "cd client && npm run build",
    "dev:migrate": "./node_modules/.bin/env-cmd ./node_modules/.bin/sequelize db:migrate --env development --config ./configs/sequelize.js",
    "lint:fix": "./node_modules/.bin/eslint --fix --format codeframe .",
    "dev:client:test": "cd client && npm test",
    "dev:server:test": "./node_modules/.bin/mocha tests/**/*.test.js",
    "lint": "npm run lint:pug && npm run lint:eslint",
    "lint:pug": "./node_modules/.bin/pug-lint .",
    "lint:eslint": "./node_modules/.bin/eslint --format codeframe ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shane-Tierney/marvel-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Shane-Tierney/marvel-api/issues"
  },
  "homepage": "https://github.com/Shane-Tierney/marvel-api#readme",
  "devDependencies": {
    "concurrently": "^7.2.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.16.0",
    "nodemon": "^2.0.16",
    "pug-lint": "^2.6.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "mysql2": "^2.3.3",
    "pug": "^3.0.2",
    "sequelize": "^6.21.2",
    "sequelize-cli": "^6.4.1"
  }
}
